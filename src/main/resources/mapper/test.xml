<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.littlebank.sample.mybatis.dao.TestDAO">

    <!-- 조회 -->
    <select id="listBank" resultType="com.project.littlebank.sample.mybatis.dto.TestEntity">
        select B_SEQ
             , B_ID
             , B_PWD
             , B_NAME
             , B_POSITION
             , B_TEL
             , USER_USE_YN
             , B_LAST_USE_TIME
             , REG_ID
             , REG_DATE
             , CHG_ID
             , CHG_DATE
             , DEPT
             , BASIC_AUTH
        from TESTBANKER
    </select>
</mapper>


